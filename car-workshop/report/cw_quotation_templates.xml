<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!--Translatable Quotation Template-->
    <template id="report_quotation_document">
        <t t-set="doc" t-value="doc.with_context({'lang':doc.partner_id.lang})" />
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <!--HEAD cw_head_templates.xml-->
                        <h2>PRESUPUESTO (Esta en Espa√±ol)</h2>
                        <t t-call="car-workshop.report_head_vehicle_customer"/>

                    </div>
                </t>
            </t>
        </t>
    </template>

        <!--Quotation Main Template-->
    <template id="report_quotation">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="car-workshop.report_quotation_document" t-lang="doc.partner_id.lang"/></t>
        </t>
    </template>

    </data>
</odoo>